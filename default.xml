<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- 定义远程仓库的基础地址 -->
  <remote name="origin"
          fetch="https://github.com/itlhd/" />
  
  <!-- 默认配置：所有项目使用 'origin' 远程，分支为 'main'，同步线程数为4 -->
  <default remote="origin" revision="main" sync-j="4"/>
  
  <!-- 具体定义您的三个项目 -->
  <project name="rkbin" path="rkbin" />
  <project name="kernel" path="kernel" />
  <project name="u-boot" path="u-boot" />
</manifest>
